    "# ds = ws.datastores[\"breeds\"]\n",
    "# need to create the datastore object like this\n",
    "ds = Datastore(ws,'breeds')\n",
    "path_on_datastore = 'breeds-10'\n",
    "ds_data = ds.path(path_on_datastore)\n",
    "print(ds_data)"
   ]
