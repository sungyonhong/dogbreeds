    "container_registry_name=                   # <<please retrieve from URL at above paragraph>>\n",
    "workspace_name=                            # <<a Unique Name -- use your alias>>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "from azureml.core import Workspace\n",
    "\n",
    "location='westeurope'\n",
